{
  "kind": "collectionType",
  "collectionName": "vouchers_categories",
  "info": {
    "singularName": "vouchers-category",
    "pluralName": "vouchers-categories",
    "displayName": "Vouchers  Category"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "vouchers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::voucher.voucher",
      "mappedBy": "vouchers_category"
    }
  }
}
